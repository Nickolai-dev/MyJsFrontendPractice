{% extends 'base.njk' %}
{% import 'macros/basic-form-element-macros.njk' as bc %}
{% block current_path %}
  Home:index.html/Basic element:basic-form-element.html
{% endblock %}
{% block body %}
<div class="row pt-3_4">
<div class="col-lg-7 mb-4">
  <div class="sheet h-100">
    <form action="#" class="basic-login-form">
      <div class="h4 font-weight-bold">Basic Login Form</div>
      <div class="d-flex flex-wrap mb-3">
        <a href="#" class="social social_facebook mr-2"><span class="social__iwrap"><i class="social__icon fa fa-facebook"></i></span><span class="social__text">Facebook</span></a>
        <a href="#" class="social social_twitter mr-2"><span class="social__iwrap"><i class="social__icon fa fa-twitter"></i></span><span class="social__text">Twitter</span></a>
        <a href="#" class="social social_google-plus mr-2"><span class="social__iwrap"><i class="social__icon fa fa-google-plus"></i></span><span class="social__text">Google+</span></a>
        <a href="#" class="social social_linkedin mr-2"><span class="social__iwrap"><i class="social__icon fa fa-linkedin"></i></span><span class="social__text">Linkedin</span></a>
      </div>
      <div class="row">
        <div class="col-lg-6">
          <div class="d-flex flex-column">
            <h3 class="h3 font-weight-bold">Sign In</h3>
            <p class="basic-login-form__info-text">Register User can get sign in from here</p>
            <label for="enterEmail" class="basic-login-form__info-text font-weight-bold">Email</label>
            <input type="email" class="basic-login-form__input mb-3" placeholder="Enter Email" id="enterEmail">
            <label for="enterPassword" class="basic-login-form__info-text font-weight-bold">Password</label>
            <input type="password" class="basic-login-form__input mb-3" placeholder="Password" id="enterPassword">
            <div class="d-flex justify-content-between align-items-center">
              <div class="d-flex align-items-center">
                <div class="mr-1">{{ bc.checkbox('rememberMe') }}</div>
                <label class="basic-login-form__info-text font-weight-bold m-0" for="rememberMe">Remember Me</label></div>
              <button type="submit" class="btn btn-material-red">Log In</button>
            </div>
          </div>
        </div>
        <div class="col-lg-6">
          <h3 class="h3 font-weight-bold">Not a member?</h3>
          <p class="basic-login-form__info-text">You can create an account:</p>
          <a href="#" class="basic-login-form__create-account-button w-100">
            <i class="fa fa-sign-in"></i></a>
        </div>
      </div>
    </form>
  </div>
</div>
